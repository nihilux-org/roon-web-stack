{
  "name": "@nihilux/roon-web-stack",
  "version": "0.0.8",
  "description": "a web stack, from api to web client, to use roon ia a browser",
  "private": true,
  "author": "nihil@nihilux.org",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "app/*"
  ],
  "engines": {
    "node": ">= 20.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nihilux-org/roon-web-stack.git"
  },
  "scripts": {
    "build": "yarn workspaces foreach -At run build",
    "lint": "yarn workspaces foreach -At run lint",
    "lint:fix": "yarn workspaces foreach -At run lint:fix",
    "test": "yarn workspaces foreach -At run test",
    "backend": "yarn workspace @nihilux/roon-web-api start",
    "frontend": "yarn workspace @nihilux/roon-web-ng-client start"
  },
  "packageManager": "yarn@4.1.1+sha256.f3cc0eda8e5560e529c7147565b30faa43b4e472d90e8634d7134a37c7f59781"
}
